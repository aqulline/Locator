                pos_hint:{"center_x":.243,"center_y":.53}
            MDLabel:
                text:"Sub-ward:"
                halign:"center"
                font_size: "20sp"
                theme_text_color: "Custom"
                font_name: 'components/fonts/Gill Sans.otf'
